			"name": "user login iam",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Check status code\", function(){",
							"    pm.expect(pm.response.code).to.eq(200);",
							"});",
							"",
							"pm.environment.set('BearerToken',pm.response.json().result.token_type+\" \"+pm.response.json().result.access_token);",
